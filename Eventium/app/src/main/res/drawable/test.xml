<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="24dp"
            android:height="24dp"
            android:viewportWidth="24"
            android:viewportHeight="24">
            <path
                android:name="dot_1"
                android:pathData="M 4 4.5 C 3.17 4.5 2.5 5.17 2.5 6 C 2.5 6.83 3.17 7.5 4 7.5 C 4.83 7.5 5.5 6.83 5.5 6 C 5.5 5.17 4.83 4.5 4 4.5 Z"
                android:fillColor="#FFFFFF"
                android:fillAlpha="0"/>
            <path
                android:name="dot_2"
                android:pathData="M 4 10.5 C 3.17 10.5 2.5 11.17 2.5 12 C 2.5 12.83 3.17 13.5 4 13.5 C 4.83 13.5 5.5 12.83 5.5 12 C 5.5 11.17 4.83 10.5 4 10.5 Z"
                android:fillColor="#FFFFFF"
                android:fillAlpha="0"/>
            <path
                android:name="dot_3"
                android:pathData="M 4 16.5 C 3.17 16.5 2.5 17.18 2.5 18 C 2.5 18.82 3.18 19.5 4 19.5 C 4.82 19.5 5.5 18.82 5.5 18 C 5.5 17.18 4.83 16.5 4 16.5 Z"
                android:fillColor="#FFFFFF"
                android:fillAlpha="0"/>
            <path
                android:name="line_1"
                android:pathData="M 7 6 L 21 6"
                android:strokeColor="#FFFFFF"
                android:strokeWidth="2"
                android:trimPathEnd="0"/>
            <path
                android:name="line_2"
                android:pathData="M 7 12 L 21 12"
                android:fillColor="#FFFFFF"
                android:strokeColor="#FFFFFF"
                android:strokeWidth="2"
                android:trimPathEnd="0"/>
            <path
                android:name="line_3"
                android:pathData="M 7 18 L 21 18"
                android:fillColor="#FFFFFF"
                android:strokeColor="#FFFFFF"
                android:strokeWidth="2"
                android:trimPathEnd="0"/>
        </vector>
    </aapt:attr>
    <target android:name="dot_1">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="fillAlpha"
                android:startOffset="100"
                android:duration="100"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
    <target android:name="dot_2">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="fillAlpha"
                android:startOffset="500"
                android:duration="100"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
    <target android:name="dot_3">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="fillAlpha"
                android:startOffset="900"
                android:duration="100"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
    <target android:name="line_1">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="trimPathEnd"
                android:startOffset="200"
                android:duration="300"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
    <target android:name="line_2">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="trimPathEnd"
                android:startOffset="600"
                android:duration="300"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
    <target android:name="line_3">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="trimPathEnd"
                android:startOffset="1000"
                android:duration="300"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
</animated-vector>
